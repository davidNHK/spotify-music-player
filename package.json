{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "axios": "0.20.0",
    "nanoid": "2.1.11",
    "parse-ms": "2.1.0",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "styled-components": "5.2.0",
    "swr": "0.3.2"
  },
  "devDependencies": {
    "@busybox/eslint-config": "5.1.1",
    "@busybox/npm-package-json-lint-config": "1.1.3",
    "@busybox/prettier-config": "1.1.3",
    "@busybox/renovate-config": "1.1.3",
    "@busybox/tsconfig": "1.3.0",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@lhci/cli": "0.5.0",
    "@pollyjs/adapter-node-http": "5.0.0",
    "@pollyjs/core": "5.0.0",
    "@pollyjs/persister-fs": "5.0.0",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.0.2",
    "@testing-library/user-event": "12.1.3",
    "@types/enzyme": "3.10.6",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.2",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "26.0.13",
    "@types/js-cookie": "2.2.6",
    "@types/nanoid": "2.1.0",
    "@types/node": "13.11.0",
    "@types/pollyjs__adapter-node-http": "2.0.0",
    "@types/pollyjs__core": "4.3.0",
    "@types/pollyjs__persister-fs": "2.0.0",
    "@types/prettier": "2.1.0",
    "@types/ramda": "0.27.15",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/setup-polly-jest": "0.5.0",
    "@types/styled-components": "5.1.3",
    "@types/stylelint": "9.10.1",
    "@types/testing-library__jest-dom": "5.9.2",
    "@types/testing-library__react": "10.0.1",
    "commitizen": "4.2.1",
    "cz-conventional-changelog": "3.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "eslint": "7.8.1",
    "husky": "4.3.0",
    "lint-staged": "10.3.0",
    "npm-package-json-lint": "5.1.0",
    "prettier": "2.1.1",
    "quicktype": "15.0.258",
    "serve": "11.3.2",
    "setup-polly-jest": "0.9.1",
    "stylelint": "13.7.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "typescript": "4.0.2",
    "typesync": "0.7.0"
  },
  "engines": {
    "node": ">=12"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:js && CI=true yarn test --coverage --watchAll=false",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "clover",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 84,
        "lines": 93,
        "statements": 90
      }
    }
  },
  "license": "MIT",
  "name": "spotify-web-player",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "lint:js": "eslint --ext=ts,tsx,json,js --fix .",
    "lint:package-json": "yarn npmPkgJsonLint .",
    "quicktype": "quicktype --just-types",
    "serve:lhci": "serve -l 42069 -s build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:reset": "REACT_APP_POLLY_MODE=record yarn test --watchAll --coverage"
  },
  "version": "0.1.0"
}
