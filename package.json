{
  "name": "spotify-web-player",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "js-cookie": "2.2.1",
    "nanoid": "2.1.11",
    "ramda": "0.27.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "5.1.1",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "swr": "0.3.0"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:reset": "REACT_APP_POLLY_MODE=record yarn test --watchAll --coverage",
    "eject": "react-scripts eject",
    "lint:js": "eslint --ext=ts,tsx --fix .",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "quicktype": "quicktype --just-types",
    "serve:lhci": "serve -l 42069 -s build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@lhci/cli": "0.4.3",
    "@pollyjs/adapter-node-http": "5.0.0",
    "@pollyjs/core": "5.0.0",
    "@pollyjs/persister-fs": "5.0.0",
    "@spotify/eslint-config": "8.0.2",
    "@spotify/eslint-config-react": "8.0.2",
    "@spotify/prettier-config": "8.0.0",
    "@spotify/tsconfig": "8.0.0",
    "@testing-library/jest-dom": "5.11.2",
    "@testing-library/react": "10.4.7",
    "@testing-library/user-event": "12.1.0",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "7.2.0",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "26.0.8",
    "@types/js-cookie": "2.2.6",
    "@types/nanoid": "2.1.0",
    "@types/node": "13.11.0",
    "@types/pollyjs__adapter-node-http": "2.0.0",
    "@types/pollyjs__core": "4.3.0",
    "@types/pollyjs__persister-fs": "2.0.0",
    "@types/prettier": "2.0.2",
    "@types/ramda": "0.27.14",
    "@types/react": "16.9.44",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/setup-polly-jest": "0.5.0",
    "@types/styled-components": "5.1.1",
    "@types/stylelint": "9.10.1",
    "@types/testing-library__jest-dom": "5.9.2",
    "@types/testing-library__react": "10.0.1",
    "@typescript-eslint/parser": "3.7.1",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "quicktype": "15.0.249",
    "serve": "11.3.2",
    "setup-polly-jest": "0.9.1",
    "stylelint": "13.6.1",
    "typescript": "3.9.7",
    "typesync": "0.7.0"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:js && CI=true yarn test --coverage --watchAll=false"
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "text-summary",
      "lcov",
      "clover",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 94,
        "branches": 90,
        "functions": 87,
        "lines": 94
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
