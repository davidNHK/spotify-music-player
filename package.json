{
  "name": "spotify-web-player",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.19.2",
    "js-cookie": "2.2.1",
    "nanoid": "2.1.11",
    "ramda": "0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.0",
    "styled-components": "5.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "swr": "0.2.0"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:reset": "REACT_APP_POLLY_MODE=record yarn test --watchAll --coverage",
    "eject": "react-scripts eject",
    "lint:js": "eslint --ext=ts,tsx --fix .",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "quicktype": "quicktype --just-types",
    "serve:lhci": "serve -l 42069 -s build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@lhci/cli": "0.3.14",
    "@pollyjs/adapter-node-http": "4.0.4",
    "@pollyjs/core": "4.0.4",
    "@pollyjs/persister-fs": "4.3.0",
    "@spotify/eslint-config": "7.0.1",
    "@spotify/eslint-config-react": "7.0.1",
    "@spotify/prettier-config": "7.0.0",
    "@spotify/tsconfig": "7.0.0",
    "@testing-library/jest-dom": "5.8.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.3.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/eslint": "6.8.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "25.2.3",
    "@types/js-cookie": "2.2.6",
    "@types/nanoid": "2.1.0",
    "@types/node": "13.11.0",
    "@types/pollyjs__adapter-node-http": "2.0.0",
    "@types/pollyjs__core": "4.0.0",
    "@types/pollyjs__persister-fs": "2.0.0",
    "@types/prettier": "2.0.0",
    "@types/ramda": "0.27.6",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/setup-polly-jest": "0.5.0",
    "@types/styled-components": "5.1.0",
    "@types/stylelint": "9.10.1",
    "@types/testing-library__jest-dom": "5.7.0",
    "@types/testing-library__react": "10.0.1",
    "@typescript-eslint/parser": "2.34.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.13.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "quicktype": "15.0.235",
    "serve": "11.3.0",
    "setup-polly-jest": "0.7.0",
    "stylelint": "13.5.0",
    "typescript": "3.9.3",
    "typesync": "0.6.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint:js && CI=true yarn test --coverage --watchAll=false"
    }
  },
  "jest": {
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 95,
        "branches": 90,
        "functions": 92,
        "lines": 95
      }
    }
  }
}
